                                        </div>
                                        <div role="separator" class="divider col-12"></div>
                                        <div class="col-12 text-left">
                                            <a href="@Url.Action("Logoff", "Account" )"><i class="fa fa-power-off"></i> Logout</a>
                                        </div>
                                    </div>
                                    <!-- /.row -->
                                </li>
                            </ul>
                        </li>
                        <!-- Control Sidebar Toggle Button -->
                        <li>
                            <a href="#" data-toggle="control-sidebar"><i class="fa fa-cog fa-spin"></i></a>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>

        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar">
            <!-- sidebar -->
            <section class="sidebar">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="ulogo">
                        <a href="@Url.Action("Index", "Home" )">
                            <!-- logo for regular state and mobile devices -->
                            <span><b>LOAN </b>APP</span>
                        </a>
                    </div>
                    <div class="image">
                        <img src="~/default-picture.png" class="rounded-circle img1" alt="User Image">
                    </div>
                    <div class="info">
                        <p><span>@Web.Application.LoggedInUser.DisplayName</span></p>
                        <a href="#" class="link" data-toggle="tooltip" title="" data-original-title="Settings"><i class="fa fa-key"></i></a>
                        <a href="#" class="link" data-toggle="tooltip" title="" data-original-title="Email"><i class="fa fa-suitcase"></i></a>
                        <a href="#" class="link" data-toggle="tooltip" title="" data-original-title="Logout"><i class="fa fa-sign-out"></i></a>
                    </div>
                </div>
                <!-- sidebar menu -->
                @Html.Partial("_MenuPartial")

            </section>
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
